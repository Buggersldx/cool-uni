<view class="cl-message__wrap"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cl-message',[(item.visible)?'is-show':'']]}}"><block wx:if="{{item.type}}"><icon class="cl-message__icon" type="{{item.type}}" size="{{item.iconSize}}"></icon></block><block wx:if="{{$slots.default}}"><slot></slot></block><block wx:else><text class="cl-message__content">{{item.message}}</text></block></view></block></view>